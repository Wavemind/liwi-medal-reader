<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.liwi_native">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.BLUETOOTH"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.INTERNET"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.RESULTS.GRANTED"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- Compliant -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.VIBRATE"/> <!-- Compliant -->
    <uses-permission android:name="android.permission.CAMERA" /> <!-- Compliant -->

    <application
      android:name=".MainApplication"
      android:label="@string/app_name"
      android:icon="@mipmap/ic_launcher"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:allowBackup="false"
      android:usesCleartextTraffic="true"
      android:theme="@style/SplashTheme">
      <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:screenOrientation="userPortrait"
        android:configChanges="keyboard|keyboardHidden|orientation|screenSize|uiMode"
        android:launchMode="singleTask"
        android:windowSoftInputMode="adjustPan">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
      </activity>
    </application>
</manifest>
